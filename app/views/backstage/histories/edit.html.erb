<h1>Editing hisory</h1>

<% form_for([:backstage, @history]) do |f| %>
  <%= f.error_messages %>
  <p>
    <label>Título</label>
    <%= f.text_field :title %>
  </p>
  <p>
    <label>Conteúdo</label>
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Back', backstage_histories_path %>
